{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "no-confusing-arrow": 0,
    "comma-dangle": 0,
    "object-curly-spacing": 0,
    "no-use-before-define": 0,
    "max-len": 0,
    "react/no-multi-comp": 0,
    "import/default": 0,
    "import/no-duplicates": 0,
    "import/named": 0,
    "import/namespace": 0,
    "import/no-unresolved": 0
  },
  "env": {
    "node": true,
    "es6": true,
    "browser": true,
    "mocha": true
  },
  "plugins": [
    "react"
  ]
}
